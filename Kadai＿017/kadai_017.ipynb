        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def check_adult(self):\n",
        "    if self.age >= 20:\n",
        "      print(\"大人です\")\n",
        "    else:\n",
        "      print(\"未成年です\")\n",
        "\n",
        "user = Human(\"太郎\", 36)\n",
        "\n",
        "print(user.age)\n",
        "\n",
        "user.check_adult()\n",
        "\n",
        "\n",
        "\n"
